<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>


  <system.serviceModel>

    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IFileService" maxReceivedMessageSize="2147483647">
          <security mode="None"/>
        </binding>
      </basicHttpBinding>
      <netTcpBinding>
        <binding name="NetTcpBinding_IMyService" closeTimeout="00:00:15"
          openTimeout="00:00:15" receiveTimeout="00:00:15" sendTimeout="00:00:15">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IMyService"
        contract="ServiceFunctions.IService" name="NetTcpBinding_IMyService" />
      <endpoint address="http://localhost:8001/" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IFileService" contract="FileServiceClient.IFileService"
        name="BasicHttpBinding_IFileService" />
    </client>

  </system.serviceModel>
</configuration>